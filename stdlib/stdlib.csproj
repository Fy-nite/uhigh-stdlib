<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <OutputType>Library</OutputType>
        <TargetFramework>net9.0</TargetFramework>
        <Nullable>enable</Nullable>
        <ImplicitUsings>enable</ImplicitUsings>
        <PackageId>uhigh-stdlib</PackageId>
        <Version>1.1.0</Version>
        <Authors>charlie-san</Authors>
        <Company>Finite</Company>
        <Description>Micro-High's official Standard Library For dotnet</Description>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <PackageProjectUrl>https://github.com/fy-nite/uhigh-stdlib</PackageProjectUrl>
        <RepositoryUrl>https://github.com/fy-nite/uhigh-stdlib</RepositoryUrl>
        <Copyright>Copyright Â© 2024 Finite</Copyright>
        <PackageReadmeFile>README.md</PackageReadmeFile>
            <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    </PropertyGroup>
    <ItemGroup>
    <None Include="../README.md" Pack="true" PackagePath="\"/>
    </ItemGroup>
</Project>